{
    "name": "modules-garden/proxmox-proxy",
    "description": "",
    "license": "EULA",
    "type": "project",
    "require": {
        "php": ">=7.2.0",
        "laravel/lumen-framework": "5.8.*",
        "vlucas/phpdotenv": "^3.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
